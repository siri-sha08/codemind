class Bank {
    String name;
    int noOfCustomers;
    double getInterest() {
        return 0.0;
    }
}
class SBI extends Bank {
    double getInterest() {
        return 7.5;
    }
}
class Canara extends Bank {
    double getInterest() {
        return 8.2;
    }
}
public class OverRiding {
    public static void main(String[] args) {
        Canara c1 = new Canara();
        System.out.println(c1.getInterest());
        SBI s1 =  new SBI();
        System.out.println(s1.getInterest());
    }
} 
